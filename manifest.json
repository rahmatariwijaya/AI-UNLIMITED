  "id": "tanya-ari-aja",
  "description": "AI yang siap membantu menjawab pertanyaanmu dengan cepat, akurat, dan gratis.",
  "background_color": "#0b111e",
  "theme_color": "#0b111e",
  "icons": [
    {
      "src": "unnamed_copy_192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "unnamed_copy_512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "unnamed_copy_512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "unnamed_copy_512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Tampilan Desktop Tanya Ari Aja"
    },
    {
      "src": "unnamed_copy_512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Tampilan Mobile Tanya Ari Aja"
    }
  ],
  "start_url": "/",
  "dir": "ltr",
  "scope": "/",
  "lang": "id",
  "orientation": "portrait",
  "display": "standalone",
  "display_override": [
    "tabbed",
    "window-controls-overlay",
    "standalone",
    "minimal-ui"
  ],
  "categories": [
    "productivity",
    "education"
  ],
  "shortcuts": [
    {
      "name": "Mulai Chat Baru",
      "short_name": "Chat Baru",
      "url": "/?action=new_chat",
      "icons": [
        {
          "src": "unnamed_copy_192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/plain": [".txt"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+tanyaari",
      "url": "/?url=%s"
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "widgets": [
    {
      "name": "Tanya Ari Widget",
      "short_name": "Tanya Ari",
      "description": "Widget cepat untuk Tanya Ari",
      "tag": "tanya-ari-widget",
      "template": "widget-template",
      "ms_ac_template": "widgets/template.json",
      "data": "widgets/data.json",
      "type": "application/json",
      "screenshots": [
        {
          "src": "unnamed_copy_512x512.png",
          "sizes": "512x512",
          "type": "image/png",
          "label": "Preview Widget"
        }
      ],
      "icons": [
        {
          "src": "unnamed_copy_192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ],
      "auth": false,
      "update": 86400
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "note_taking": {
    "new_note_url": "/new-note"
  }
}
